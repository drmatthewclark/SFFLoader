
-- update before indexing
UPDATE reaxys.sff  set molecule = mol_from_smiles(smiles::cstring);

ALTER TABLE ONLY reaxys.sff
    ADD CONSTRAINT sff_pkey PRIMARY KEY (xrn);

CREATE INDEX sff_mol_index ON reaxys.sff USING gist (molecule);
CREATE INDEX sff_inchi_index on reaxys.sff (inchi);
CREATE INDEX sff_cn_index on reaxys.sff(cn);
