--
-- Name: molecule; Type: TABLE; Schema: reaxys; Owner: -
--
drop schema if exists reaxys_sff cascade;
create schema reaxys_sff

create table reaxys_sff.version(
 load_date TIMESTAMP WITH TIME ZONE DEFAULT CURRENT_TIMESTAMP,
 version text
);

CREATE TABLE reaxys_sff.sff  (
    XRN bigint NOT NULL,
    smiles text,
    sdfile text,
    CN text,
    MW real,
    MF text,
    RN text,
    INCHI text,
    FA text,
    STYPE text,
    rx_file_id integer,
    molecule public.mol
)
WITH (autovacuum_enabled='false');

